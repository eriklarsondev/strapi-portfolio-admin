{
  "kind": "collectionType",
  "collectionName": "team_members",
  "info": {
    "singularName": "team-member",
    "pluralName": "team-members",
    "displayName": "Team Member"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "reachable": {
      "type": "boolean",
      "default": false
    },
    "image": {
      "type": "media",
      "allowedTypes": ["images"]
    },
    "fullName": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "slug": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "links": {
      "type": "component",
      "repeatable": true,
      "component": "menu.menu-item"
    },
    "content": {
      "type": "customField",
      "options": {
        "output": "HTML",
        "preset": "light"
      },
      "customField": "plugin::ckeditor.CKEditor"
    },
    "articles": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::article.article",
      "mappedBy": "author"
    },
    "seo": {
      "type": "component",
      "component": "meta.seo"
    }
  }
}
