{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "singularName": "article",
    "pluralName": "articles",
    "displayName": "Article"
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "slug": {
      "type": "string"
    },
    "excerpt": {
      "type": "text"
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "inversedBy": "articles"
    },
    "content": {
      "type": "customField",
      "options": {
        "output": "HTML",
        "preset": "light"
      },
      "customField": "plugin::ckeditor.CKEditor"
    },
    "author": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::team-member.team-member",
      "inversedBy": "articles"
    },
    "seo": {
      "type": "component",
      "component": "meta.seo"
    }
  }
}
